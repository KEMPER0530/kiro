{
    "name": "youtube-eafc-player-infrastructure",
    "version": "1.0.0",
    "description": "AWS CDK infrastructure for YouTube eFootball Player",
    "main": "lib/index.js",
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "test": "jest",
